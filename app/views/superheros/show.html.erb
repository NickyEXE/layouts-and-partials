<%= render partial:'superhero', locals: {superhero: @superhero} %>
<% if @superhero.team %>
<p>Team: <%= @superhero.team.name %></p>
<% end %>

<%= button_to "Edit #{@superhero.name}", edit_superhero_path, method: :get %>
<%= button_to "Delete #{@superhero.name}", superhero_path, method: :delete %>
