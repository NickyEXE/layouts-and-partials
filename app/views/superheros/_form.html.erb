<%= form_for @superhero do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :alias %>
  <%= f.text_field :alias %><br>
  <%= f.label :power %>
  <%= f.text_field :power %><br>
  <%= f.label :power_level %>
  <%= f.number_field :power_level %><br>
  <%= f.label :team_id %>
  <%= f.collection_select :team_id, Team.all, :id, :name %><br><br>
  <%= f.submit %>
<% end %>
